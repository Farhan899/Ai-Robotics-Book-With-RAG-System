{
  "name": "ai-robotics-book-backend",
  "version": "1.0.0",
  "description": "Backend server for the AI Robotics Book chat widget",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:python": "cd src && python -m api.main",
    "dev:both": "concurrently \"npm run start\" \"npm run start:python\" --kill-others-on-fail"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ai",
    "robotics",
    "chat",
    "docusaurus"
  ],
  "author": "AI Robotics Book",
  "license": "MIT"
}
